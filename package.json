{
  "name": "@aptuitiv/rets-client",
  "version": "0.1.0",
  "description": "NodeJS library for interfacing with RETS servers",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "lint": "eslint --fix --ext .js,.ts .",
    "build": "esbuild ./src/index.ts --outdir=dist --bundle --format=esm",
    "build2": "node scripts/build.js",
    "watch": "node scripts/watch.js",
    "dev": "esbuild ./src/index.ts --outdir=dist --bundle --format=esm --watch",
    "test": "mocha",
    "ts-build": "tsc",
    "ts-dev": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aptuitiv/rets-client3.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aptuitiv/rets-client3/issues"
  },
  "homepage": "https://github.com/aptuitiv/rets-client3#readme",
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/mocha": "^9.1.1",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "chai": "^4.3.6",
    "chalk": "^5.0.1",
    "esbuild": "^0.15.7",
    "esbuild-plugin-eslint": "^0.1.1",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "log-symbols": "^5.1.0",
    "mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
